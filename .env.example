# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key
SUPABASE_JWT_SECRET=your-jwt-secret

# JWT Configuration
SUPABASE_JWT_ALGORITHM=HS256
SUPABASE_JWT_LEEWAY=60
SUPABASE_JWT_TTL=3600
SUPABASE_JWT_REFRESH_TTL=604800

# Authentication Settings
SUPABASE_AUTH_REDIRECT_URL=http://localhost:8000/auth/callback
SUPABASE_AUTH_PROVIDER_REDIRECT=/dashboard
SUPABASE_AUTH_LOGOUT_REDIRECT=/
SUPABASE_AUTH_SESSION_TIMEOUT=3600
SUPABASE_AUTH_AUTO_REFRESH=true
SUPABASE_AUTH_REMEMBER_DURATION=2419200

# Security Configuration
SUPABASE_PASSWORD_MIN_LENGTH=8
SUPABASE_PASSWORD_REQUIRE_UPPERCASE=false
SUPABASE_PASSWORD_REQUIRE_LOWERCASE=false
SUPABASE_PASSWORD_REQUIRE_NUMBERS=false
SUPABASE_PASSWORD_REQUIRE_SYMBOLS=false
SUPABASE_ENCRYPTION_ALGORITHM=AES-256-GCM
SUPABASE_KEY_ROTATION_DAYS=90
SUPABASE_CSRF_PROTECTION=true
SUPABASE_SECURE_COOKIES=true
SUPABASE_SAME_SITE=lax

# Rate Limiting
SUPABASE_RATE_LIMITING_ENABLED=true
SUPABASE_LOGIN_MAX_ATTEMPTS=5
SUPABASE_LOGIN_DECAY_MINUTES=15
SUPABASE_REGISTER_MAX_ATTEMPTS=3
SUPABASE_REGISTER_DECAY_MINUTES=60
SUPABASE_PASSWORD_RESET_MAX_ATTEMPTS=3
SUPABASE_PASSWORD_RESET_DECAY_MINUTES=30
SUPABASE_OTP_MAX_ATTEMPTS=3
SUPABASE_OTP_DECAY_MINUTES=5

# Circuit Breaker
SUPABASE_CIRCUIT_BREAKER_ENABLED=true
SUPABASE_CB_FAILURE_THRESHOLD=5
SUPABASE_CB_RECOVERY_TIMEOUT=60

# Caching
SUPABASE_CACHE_ENABLED=true
SUPABASE_CACHE_STORE=redis
SUPABASE_CACHE_USER_TTL=300
SUPABASE_CACHE_JWT_TTL=60
SUPABASE_CACHE_OAUTH_TTL=600
SUPABASE_CACHE_PREFIX=supabase_auth
SUPABASE_CACHE_COMPRESSION=true

# HTTP Client
SUPABASE_HTTP_TIMEOUT=10.0
SUPABASE_HTTP_CONNECT_TIMEOUT=5.0
SUPABASE_HTTP_RETRY_ATTEMPTS=3
SUPABASE_HTTP_RETRY_DELAY=1000
SUPABASE_HTTP_VERIFY_SSL=true
SUPABASE_HTTP_USER_AGENT=Supabase-Laravel-Auth/1.0
SUPABASE_HTTP_POOL_SIZE=10

# Monitoring
SUPABASE_MONITORING_ENABLED=true
SUPABASE_METRICS_ENABLED=true
SUPABASE_METRICS_DRIVER=prometheus
SUPABASE_METRICS_NAMESPACE=supabase_auth
SUPABASE_LOG_CHANNEL=stack
SUPABASE_LOG_LEVEL=info
SUPABASE_HEALTH_CHECKS_ENABLED=true
SUPABASE_HEALTH_ENDPOINT=/health/supabase
SUPABASE_HEALTH_INTERVAL=30

# OAuth Providers
SUPABASE_OAUTH_GOOGLE_ENABLED=false
SUPABASE_OAUTH_GOOGLE_CLIENT_ID=
SUPABASE_OAUTH_GOOGLE_REDIRECT=

SUPABASE_OAUTH_GITHUB_ENABLED=false
SUPABASE_OAUTH_GITHUB_CLIENT_ID=
SUPABASE_OAUTH_GITHUB_REDIRECT=

SUPABASE_OAUTH_DISCORD_ENABLED=false
SUPABASE_OAUTH_DISCORD_CLIENT_ID=
SUPABASE_OAUTH_DISCORD_REDIRECT=

SUPABASE_OAUTH_STATE_TTL=600
SUPABASE_OAUTH_PKCE_ENABLED=true

# Email Verification
SUPABASE_EMAIL_VERIFICATION_ENABLED=true
SUPABASE_EMAIL_VERIFICATION_URL=http://localhost:8000/email/verify
SUPABASE_EMAIL_VERIFICATION_TTL=86400
SUPABASE_EMAIL_AUTO_VERIFY=false

# Password Reset
SUPABASE_PASSWORD_RESET_URL=http://localhost:8000/password/reset
SUPABASE_PASSWORD_RESET_TTL=3600
SUPABASE_PASSWORD_RESET_THROTTLE=60